#include <stdlib.h>
#include <time.h>
#include <stdio.h>
/* more headers goes there */

/* betty style doc for function main goes there */
/**
 *main - Entry point
 *
 *Return: Always 0 (Success)
 */
int main(void)
{
	int n;
	int x = n % 10;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	/* your code goes there */
	if (x > 5)
	{
		printf("Last digit of %d is %d\n", n, x);
	}
	else if (x == 0)
	{
		printf("Last dogot of %d is %d\n", n, x);
	}
	else if (x < 6)
	{
		printf("Last digit of %d is %d\n", n, x);
	}
	return (0);
}
